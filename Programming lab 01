#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a;
    scanf("%d",&a);
    printf("%d",(12*a));

    return 0;
}

ques:5

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,d1,d2;
    scanf("%d%d%d",&a,&d1,&d2);
    if ((a%d1==0)&&(a%d2==0))
    {
        printf("Yes.");
    }
    else
    {
        printf("No.");
    }

    return 0;
}

ques:6

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      char c;
    int a;
    scanf("%c",&c);
    a=("%d",c);
    if (a==65||a==69||a==73||a==79||a==85||a==97||a==101||a==105||a==111||a==117)
        printf("Vowel.");
    else if (a>=65 && a<=90 || a>=97 && a<=122)
        printf("Consonant.");
    else
        printf("Not an alphabet.");
    

    return 0;
}


#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){
    float a;
    float x,y;
    scanf("%f",&a);
    x=floor(a);
    y=ceil(a);
    printf("%f\n",x);
    printf("%f",y);
    
    return 0;

} 

