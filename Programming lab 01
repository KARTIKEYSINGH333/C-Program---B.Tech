ques:1

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char c;
    printf("");
    scanf("%c", &c);
    
    printf("%d",c);
    
    return 0;
}

ques:2

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int a;
    scanf("%d",&a);
    printf("%d\n",a);
    printf("%o\n",a);
    printf("%x\n",a);
    printf("%X",a);


    return 0;
}

ques:3
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){
    float a;
    float x,y;
    scanf("%f",&a);
    x=floor(a);
    y=ceil(a);
    printf("%f\n",x);
    printf("%f",y);
    
    return 0;

} 

