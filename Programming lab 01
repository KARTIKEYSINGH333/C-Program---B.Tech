#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      char c;
    int a;
    scanf("%c",&c);
    a=("%d",c);
    if (a==65||a==69||a==73||a==79||a==85||a==97||a==101||a==105||a==111||a==117)
        printf("Vowel.");
    else if (a>=65 && a<=90 || a>=97 && a<=122)
        printf("Consonant.");
    else
        printf("Not an alphabet.");
    

    return 0;
}

ques:7

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     int a,b;
    scanf("%d%d",&a,&b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("%d %d",a,b);
    

    return 0;
}

ques:8

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int a,b ;
    scanf("%d%d",&a,&b);
    printf("%d", a*b);
    return 0;
}

ques:9

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    float a,c,b;
    scanf("%f%f%f",&a,&b,&c);
    printf("%f\n",(pow(a,b)));
    printf("%f",(sqrt(c)));
 
    return 0;
}

